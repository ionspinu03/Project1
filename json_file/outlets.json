<script type="text/javascript">
    wrmi.scopeSelectionWizardCreateAndInit = function() {
        wrmi.scopeSelectionWizard = wrmi.scope.component.create();
        
                    wrmi.scopeSelectionWizard.registerListener('onProcessUserSelection', function(selectedScopes) {
                        wrmi.blockUI();
                        $("#reportResultInfoDiv").css('display', 'none');
                        addHiddenFormInput("reportForm", "scopes", selectedScopes.toScopeIds());
                        wrmi.form.datePicker.addHiddenFormFields("reportForm");
                        $("#reportForm").submit();
                    });
                    wrmi.scopeSelectionWizard.registerListener('onChangeUserSelection', function(selectedScopes) {
                    jQuery.ajax({type:'POST',data:'scopes=' + selectedScopes.toScopeIds(), url:'/scopeSelectionFeatureReport/getOutletDevices',success:function(data,textStatus){jQuery('#devicesSelect').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
                    });
                
        wrmi.scopeSelectionWizard.init('scopeSelectionWizardContainer', wrmi.scopeSelectionWizardData);
        

        
    };

    
        $(function() {
            wrmi.scopeSelectionWizardCreateAndInit();
        });
    
</script>

