<script type="text/javascript">
	wrmi.report.createReportGrid = function () {

	    wrmi.report.reportGrid = wrmi.slickGrid.createCommonGrid();
	    wrmi.report.reportGrid.extend({

	        options: {
	            showHeaderRow: false,
	            showFooterRow: true,
	            footerRowClass: "reportTotalRow",
	            allowSorting: false,
	            tripleStateSorting: true,
	            dataItemColumnValueExtractor: function(item, colDef) {
	                if (item.rowType != wrmi.report.ReportRowType.STANDARD && wrmi.report.reportGrid.grid.getColumnIndex(colDef.id) == 0) {
	                	var gridCols = wrmi.report.reportGrid.grid.getColumns();
	                	var totalColumn = item.rowTypeCol;
	                	var leftMarginColumn = totalColumn;
	                	var leftMargin = 0;
	                	var options = wrmi.report.reportGrid.options;

						if(options.totalsColumnExtraParameter && options.totalsColumnExtraParameter.column === totalColumn){
							leftMargin = options.totalsColumnExtraParameter.margin
						}else{
							for (var i = 0; i < gridCols.length && gridCols[i].id != leftMarginColumn; i++) {
								leftMargin += gridCols[i].width;
							}
						}
	                    var formattedTotal = item[totalColumn + "Formatted"] != null ? item[totalColumn + "Formatted"] : item[totalColumn] != null ? item[totalColumn] : "";
	                    return "<span style='margin-left: " + leftMargin + "px; width: 100%;'>" +
	                            wrmi.report.reportGrid.getTotalsLabels()[item.rowTypeCol] + " (" + formattedTotal + "):</span>";
	                }
	                var formattedField = item[colDef.field + "Formatted"];
	                return formattedField != null ? formattedField : item[colDef.field] != null ? item[colDef.field] : "";
	            }
	        },

	        getItemMetadataFn: function() {
	            var outer = this;
	            return function (row) {
		            var data = this.getItem(row);
		            if (data.rowType != wrmi.report.ReportRowType.STANDARD) {
		                return {
		                	"cssClasses": "reportTotalRow",
		                    "columns": {
		                      0: {
		                        "colspan": outer.totalRowColspan
		                      }
		                    }

		                }
		            }
                    return null;
                }
	        },

	        getFooterText: function(colId) {
	            if (this.grid.getColumnIndex(colId) == 0) {
	            	return "Grand Total:";
	            }
	            else {
                    var formattedField = this.footerData[colId + "Formatted"];
                    return formattedField != null ? formattedField : this.footerData[colId] != null ? this.footerData[colId] : "";
	            }
	        },

	        getTotalsLabels: function() {
		        if (this.totalLabels == null) {
			        var totalsLabels = [];
				    
				    	
			    	
				    	
			        		totalsLabels["businessDay"] = "Day Sum";
			        	
			    	
				    	
			        		totalsLabels["week"] = "Week Sum";
			        	
			    	
				    	
			        		totalsLabels["vlt"] = "CT Sum";
			        	
			    	
				    	
			        		totalsLabels["supplier"] = "Supplier Sum";
			        	
			    	
				    	
			        		totalsLabels["outletDisplayNumber"] = "Outlet Sum";
			        	
			    	
				    	
			        		totalsLabels["outletDscr"] = "Outlet Sum";
			        	
			    	
				    	
			        		totalsLabels["companyCode"] = "Company Sum";
			        	
			    	
				    	
			        		totalsLabels["companyDisplayName"] = "Company Sum";
			        	
			    	
				    	
			        		totalsLabels["countryCode"] = "Country Sum";
			        	
			    	
				    	
			        		totalsLabels["activityOutletDisplayNumber"] = "Outlet Sum";
			        	
			    	
				    	
			        		totalsLabels["activityCompanyCode"] = "Company Sum";
			        	
			    	
				    	
			        		totalsLabels["activityCountryCode"] = "Country Sum";
			        	
			    	
				    	
			        		totalsLabels["cashier"] = "Cashier Sum";
			        	
			    	
				    	
			        		totalsLabels["cashierAndDevice"] = "Cashier and Device Sum";
			        	
			    	
				    	
			        		totalsLabels["deviceDisplayNumber"] = "Device Sum";
			        	
			    	
				    	
			        		totalsLabels["accountEntryType"] = "Entry Type Sum";
			        	
			    	
				    	
			        		totalsLabels["gameSupplier"] = "Game Supplier Sum";
			        	
			    	
				    	
			        		totalsLabels["gameDesigner"] = "Game Designer Sum";
			        	
			    	
				    	
			        		totalsLabels["gameSet"] = "Game Set Sum";
			        	
			    	
				    	
			        		totalsLabels["game"] = "Game Sum";
			        	
			    	
				    	
			        		totalsLabels["gameVersion"] = "Game Version Sum";
			        	
			    	
				    	
			        		totalsLabels["drawId"] = "Draw Sum";
			        	
			    	
				    	
			        		totalsLabels["county"] = "County Sum";
			        	
			    	
				    	
			    	
				    	
			        		totalsLabels["promoName"] = "Promotion Name";
			        	
			    	
				    	
			        		totalsLabels["promotionName"] = "Promotion Sum";
			        	
			    	
				    	
			        		totalsLabels["currencyCode"] = "Grand Total ";
			        	
			    	
		        }
		    	return totalsLabels;
		    },

            getComparerFn: function (sortAsc) {
                var outer = this;
                return function (a, b) {
                    if (sortAsc == null) {
                        return parseInt(a.id) > parseInt(b.id) ? 1 : -1;
                    }
                    if (a.rowType === b.rowType) {
						var x = a[outer.sortcol];
						var y = b[outer.sortcol];

                    	if(x && y) {
							if (isNaN(x) || isNaN(y)) {
								x = x.toLowerCase();
								y = y.toLowerCase();
							} else {
								x = Number(x);
								y = Number(y);
							}
						}
                        return (x === y ? 0 : (x > y ? 1 : -1));
                    } else {
                        return sortAsc ? -wrmi.report.ReportRowType.compare(a.rowType, b.rowType)
                                         : wrmi.report.ReportRowType.compare(a.rowType, b.rowType);
                    }
                }
            }
	    });
	    return wrmi.report.reportGrid;
	}
</script>
