    <form onsubmit="jQuery.ajax({type:'POST',data:jQuery(this).serialize(), url:'/ctTransactionsReport/viewReport',success:function(data,textStatus){jQuery('#resultDiv').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});return false" method="post" action="/ctTransactionsReport/viewReport" id="viewReportForm">
        <input type="hidden" name="reportId" value="" id="reportId" />
    </form>

    <div id="resultDiv" style="min-width: 1300px; margin-top:20px">
    </div>

</div>

<script type="text/javascript">
    displayServerTime($("#serverTime"));

    function displayServerTime($serverTime) {
          var serverTimeMillis = 1723818835276;
          var serverTimeDiffMillis  = new Date().getTime() - serverTimeMillis;
          if($serverTime.length > 0) {
              setInterval(function() {
                  $serverTime.text(getTime(serverTimeDiffMillis));
              }, 500);
          }
      }

    function leadZeros(value, width) {
        while (String(value).length < width) {
            value = "0" + value;
        }
        return value;
    }

     function getTime(timeDiffMillis) {
        // calculate current date for display
        var now = new Date(new Date().getTime() - timeDiffMillis);
        // set the date to the time holder element
        return [
            leadZeros(now.getHours(), 2),
            leadZeros(now.getMinutes(), 2),
            leadZeros(now.getSeconds(), 2)
        ].join(":");
    }
</script>
