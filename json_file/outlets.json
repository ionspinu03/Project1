wrmi = {
	namespace: function(p_ns){
        var path = p_ns.split(".");
        var ns = window[path[0]] = window[path[0]] || {};
        for (var i = 1; i < path.length; i++) {
        	ns = ns[path[i]] = ns[path[i]] || {};
        }
        return ns;
    },

    blockUI: function() {
        $('#contentDiv').block({
            theme: true,
            fadeIn: 500,
            fadeOut: 500,
            title: "Please wait",
            message: "Waiting for response ..."
        });
    },

    unblockUI: function() {
    	$('#contentDiv').unblock();
    },

    blockDiv: function(divId) {
        $('#' + divId).block({
